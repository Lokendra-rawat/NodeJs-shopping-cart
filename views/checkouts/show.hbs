<div class="row callout cent">
	<h4>Payment Status : {{ trans.status }} </h4>
	<h2> {{ res.header }} </h2>
	<p class="secondary"> {{ res.message }} </p>

	<hr>
	<table class="striped hover stack">
		<tr>
			<th>id </th>
			<th>type </th>
			<th>amount </th>
			<th>status </th>
			<th>created at </th>
			<th>updated at </th>
		</tr>
		<tr>
			<td> {{trans.id}} </td>
			<td> {{trans.type}} </td>
			<td> {{trans.amount}} </td>
			<td> {{trans.status}} </td>
			<td> {{trans.createdAt}} </td>
			<td> {{trans.updatedAt}} </td>
		</tr>
	</table>

</div>